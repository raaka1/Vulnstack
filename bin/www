#!/usr/bin/env node

var app = require('../app'),
    tls = require('tls'),
    fs = require('fs');

options = {
    key: fs.readFileSync('bin/certs/ryans-key.pem'),
    cert: fs.readFileSync('bin/certs/ryans-cert.pem')
}
//app.set('port', process.env.PORT || 8080);
server = tls.createServer(options, app).listen(8080);